<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <SourceProxy name="GLGPUSupercurrentFilter2" class="vtkGLGPUSupercurrentFilter2" label="GLGPUSupercurrentFilter2">
      <Documentation
         short_help="compute supercurrent from GLGPU data"
         long_help="compute supercurrent from GLGPU data">
      </Documentation>
      <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData"/>
        </DataTypeDomain>
      </InputProperty>

      <Hints>
        <ShowInMenu category="TDGL"/>
      </Hints>
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
